<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ML" Version="1.7.1" />
    <PackageReference Include="Microsoft.ML.FastTree" Version="1.7.1" />
  </ItemGroup>
  <ItemGroup Label="L-BFGS">
    <None Include="L-BFGS.consumption.cs">
      <DependentUpon>L-BFGS.mbconfig</DependentUpon>
    </None>
    <None Include="L-BFGS.training.cs">
      <DependentUpon>L-BFGS.mbconfig</DependentUpon>
    </None>
    <None Include="L-BFGS.zip">
      <DependentUpon>L-BFGS.mbconfig</DependentUpon>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>